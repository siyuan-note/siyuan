				{
					"Type": "NodeTextMark",
					"TextMarkType": "code",
					"TextMarkTextContent": "."
				},
				{
					"Type": "NodeText",
					"Data": "​ 开头的路径文件属于隐藏文件，思源不会进行处理。"
				}
			]
